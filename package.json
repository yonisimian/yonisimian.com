{
  "name": "vitesome",
  "version": "1.3.0",
  "description": "A simple opinionated Vue3 Starter Template with Vite.js",
  "scripts": {
    "dev": "vite --open --host",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "update-dependencies": "ncu -u && npm install"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "vue": "^3.5.16"
  },
  "devDependencies": {
    "@iconify-json/ant-design": "^1.2.5",
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/ph": "^1.2.2",
    "@iconify/iconify": "^3.1.1",
    "@iconify/json": "^2.2.347",
    "@types/aos": "^3.0.7",
    "@types/node": "^22.15.30",
    "@unocss/preset-icons": "^66.1.4",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vueuse/core": "^13.3.0",
    "pathe": "^2.0.3",
    "typescript": "^5.8.3",
    "unocss": "^66.1.4",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^2.2.10"
  }
